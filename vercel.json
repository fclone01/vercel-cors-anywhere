{
    "version": 2,
    "headers": [
        {
            "source": "/api(.*)",
            "headers": [
                {
                    "key": "cache-control",
                    "value": "s-maxage=31536000, stale-while-revalidate"
                },
                {
                    "key": "access-control-allow-origin",
                    "value": "*"
                }
            ]
        }
    ]
}
